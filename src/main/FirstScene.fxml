<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<GridPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
          xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controller">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="107.0" minWidth="10.0" prefWidth="107.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="118.0" minWidth="10.0" prefWidth="115.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="269.0" minWidth="10.0" prefWidth="192.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="266.0" minWidth="9.0" prefWidth="82.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="87.0" minHeight="10.0" prefHeight="81.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="44.0" minHeight="0.0" prefHeight="44.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="52.0" minHeight="9.0" prefHeight="42.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="54.0" minHeight="0.0" prefHeight="25.0" vgrow="SOMETIMES"/>
