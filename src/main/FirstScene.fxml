        <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="60.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="24.0" minHeight="0.0" prefHeight="0.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label layoutX="10.0" layoutY="22.0" text="Colors:" GridPane.columnIndex="1" GridPane.rowIndex="3">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <CheckBox fx:id="checkSpades" mnemonicParsing="false" text="♠" GridPane.columnIndex="2" GridPane.rowIndex="3"/>
        <CheckBox fx:id="checkHearts" mnemonicParsing="false" text="♥" GridPane.columnIndex="2" GridPane.rowIndex="3">
            <GridPane.margin>
                <Insets left="35.0"/>
            </GridPane.margin>
        </CheckBox>
        <CheckBox fx:id="checkClubs" layoutX="270.0" layoutY="87.0" mnemonicParsing="false" text="♣"
                  GridPane.columnIndex="2" GridPane.rowIndex="3">
            <GridPane.margin>
                <Insets left="105.0"/>
            </GridPane.margin>
        </CheckBox>
        <Label text="Time to remember(s):" GridPane.columnIndex="1" GridPane.rowIndex="2">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <CheckBox fx:id="checkDiamonds" layoutX="240.0" layoutY="87.0" mnemonicParsing="false" text="♦"
                  GridPane.columnIndex="2" GridPane.rowIndex="3">
            <GridPane.margin>
                <Insets left="70.0"/>
            </GridPane.margin>
        </CheckBox>
        <ComboBox fx:id="comboBox" prefWidth="150.0" GridPane.columnIndex="2" GridPane.rowIndex="2"/>
        <Label prefHeight="73.0" prefWidth="205.0" text="Settings" GridPane.columnIndex="2">
            <font>
                <Font size="50.0"/>
            </font>
        </Label>
        <Label layoutX="117.0" layoutY="182.0" text="Concetration time(s):" GridPane.columnIndex="1" GridPane.rowIndex="4">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <Button fx:id="goButton" maxHeight="60.0" mnemonicParsing="false" onAction="#handleGoAction" prefHeight="60.0"
                prefWidth="2000.0" text="GO" GridPane.columnIndex="2" GridPane.rowIndex="8">
            <font>
                <Font name="System Bold" size="35.0"/>
            </font>
        </Button>
        <Button fx:id="instButton" mnemonicParsing="false" onAction="#handleInstAction" prefHeight="25.0" prefWidth="78.0" text="Instructions"
                GridPane.columnIndex="1" GridPane.rowIndex="8"/>
        <ComboBox fx:id="comboBox1" prefWidth="150.0" GridPane.columnIndex="2" GridPane.rowIndex="4"/>
    </children>
</GridPane>
